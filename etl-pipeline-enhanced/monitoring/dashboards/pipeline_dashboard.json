{
  "dashboard": {
    "id": null,
    "uid": "etl-pipeline-dashboard",
    "title": "ETL Pipeline Monitoring Dashboard",
    "tags": ["etl", "pipeline", "monitoring"],
    "timezone": "browser",
    "schemaVersion": 30,
    "version": 1,
    "refresh": "10s",
    "panels": [
      {
        "id": 1,
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 0},
        "type": "graph",
        "title": "Records Processed Over Time",
        "targets": [
          {
            "expr": "rate(etl_records_processed_total[5m])",
            "legendFormat": "{{pipeline}} - {{stage}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "short", "label": "Records/sec"},
          {"format": "short"}
        ]
      },
      {
        "id": 2,
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 0},
        "type": "graph",
        "title": "Processing Duration",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(etl_processing_duration_seconds_bucket[5m]))",
            "legendFormat": "p95 - {{pipeline}}",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.50, rate(etl_processing_duration_seconds_bucket[5m]))",
            "legendFormat": "p50 - {{pipeline}}",
            "refId": "B"
          }
        ],
        "yaxes": [
          {"format": "s", "label": "Duration"},
          {"format": "short"}
        ]
      },
      {
        "id": 3,
        "gridPos": {"h": 8, "w": 8, "x": 0, "y": 8},
        "type": "stat",
        "title": "Total Records Processed",
        "targets": [
          {
            "expr": "sum(etl_records_processed_total)",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "mappings": [],
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"color": "green", "value": null},
                {"color": "red", "value": 1000000}
              ]
            },
            "unit": "short"
          }
        }
      },
      {
        "id": 4,
        "gridPos": {"h": 8, "w": 8, "x": 8, "y": 8},
        "type": "stat",
        "title": "Error Rate",
        "targets": [
          {
            "expr": "rate(etl_errors_total[5m])",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "mappings": [],
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"color": "green", "value": null},
                {"color": "yellow", "value": 0.01},
                {"color": "red", "value": 0.05}
              ]
            },
            "unit": "percentunit"
          }
        }
      },
      {
        "id": 5,
        "gridPos": {"h": 8, "w": 8, "x": 16, "y": 8},
        "type": "gauge",
        "title": "Pipeline Status",
        "targets": [
          {
            "expr": "etl_pipeline_status",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "mappings": [
              {"type": "value", "value": "0", "text": "Stopped"},
              {"type": "value", "value": "1", "text": "Running"}
            ],
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"color": "red", "value": 0},
                {"color": "green", "value": 1}
              ]
            },
            "unit": "none"
          }
        }
      },
      {
        "id": 6,
        "gridPos": {"h": 8, "w": 24, "x": 0, "y": 16},
        "type": "table",
        "title": "Recent Errors",
        "targets": [
          {
            "expr": "topk(10, increase(etl_errors_total[1h]))",
            "format": "table",
            "refId": "A"
          }
        ]
      }
    ]
  }
}
